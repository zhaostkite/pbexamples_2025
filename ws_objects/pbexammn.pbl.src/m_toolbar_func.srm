event clicked;w_toolbar_func_sheet	lw_Sheet

// Save Frame toolbars
f_save_toolbar_profile(iw_Frame)

lw_Sheet = iw_Frame.GetActiveSheet()
If IsValid(lw_Sheet) Then
	// Save Sheet toolbars
	f_save_toolbar_profile(lw_Sheet)
End If

end event

