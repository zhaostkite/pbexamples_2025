table(column=(type=number update=yes updatewhereclause=yes key=yes name=emp_id dbname="employee.emp_id" validation="integer(gettext()) > 100" validationmsg="~"All employee #'s are greater than 100.~"" )
 column=(type=number update=yes updatewhereclause=yes name=dept_id dbname="employee.dept_id" validation="f_dept_lookup (GetText() )" validationmsg="'The department you entered does not exist in the department table.  Please enter another department.'" )
 column=(type=char(20) update=yes updatewhereclause=yes name=name dbname="employee.emp_lname" validation="match( gettext(), ~"^[A-Z]+[a-z]*$~" )" validationmsg="~"Name must only contain alphabetic characters.  The first character of Name must be capitalized.~"" )
 column=(type=char(1) update=yes updatewhereclause=yes name=status dbname="employee.status" initial="A" values="Active	A/Terminated	T/On Leave	L/" )
 column=(type=decimal(3) update=yes updatewhereclause=yes name=salary dbname="employee.salary" validation="real(gettext()) < If ( dept_id = 100 , 65000, If (dept_id = 200, 60000, 90000))" validationmsg="'The salary for dept ' + dept_id + ' must be less than ' + if (dept_id = 100 , '$65,000', if (dept_id = 200, '$60,000', '$90,000'))" )
 column=(type=char(1) update=yes updatewhereclause=yes name=sex dbname="employee.sex" initial="M" )
 retrieve="PBSELECT(TABLE(NAME=~"employee~" ) COLUMN(NAME=~"employee.emp_id~") COLUMN(NAME=~"employee.dept_id~") COLUMN(NAME=~"employee.emp_lname~") COLUMN(NAME=~"employee.status~") COLUMN(NAME=~"employee.salary~") COLUMN(NAME=~"employee.sex~")) " update="employee" updatewhere=1 updatekeyinplace=no )
