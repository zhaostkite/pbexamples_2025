 retrieve="PBSELECT( VERSION(400) TABLE(NAME=~"customer~" )  TABLE(NAME=~"employee~" )  TABLE(NAME=~"sales_order~" ) COLUMN(NAME=~"customer.fname~") COLUMN(NAME=~"customer.lname~") COLUMN(NAME=~"customer.company_name~") COLUMN(NAME=~"customer.address~") COLUMN(NAME=~"customer.city~") COLUMN(NAME=~"customer.state~") COLUMN(NAME=~"customer.zip~") COLUMN(NAME=~"employee.emp_fname~") COLUMN(NAME=~"employee.emp_lname~") COLUMN(NAME=~"sales_order.id~") COLUMN(NAME=~"sales_order.order_date~")    JOIN (LEFT=~"employee.emp_id~"    OP =~"=~"RIGHT=~"sales_order.sales_rep~" )    JOIN (LEFT=~"customer.id~"    OP =~"=~"RIGHT=~"sales_order.cust_id~" )WHERE(    EXP1 =~"customer.id ~"   OP =~"not in~"    EXP2 =~"(SELECT ~~~"cust_id~~~" FROM ~~~"sales_order~~~" WHERE (~~~"order_date~~~" >= :last_order_date) )~"    LOGIC =~"and~" ) WHERE(    EXP1 =~"customer.id || String(sales_order.order_date)~"   OP =~"in~"    EXP2 =~"(SELECT ~~~"cust_id~~~" || String(Max(~~~"order_date~~~")) FROM ~~~"sales_order~~~" GROUP BY ~~~"cust_id~~~")~" ) ) ORDER(NAME=~"employee.emp_lname~" ASC=yes )  ORDER(NAME=~"employee.emp_fname~" ASC=yes )  ORDER(NAME=~"customer.company_name~" ASC=yes ) ARG(NAME = ~"last_order_date~" TYPE = date) " arguments=(("last_order_date", date)) )
